generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model accounts_analyzed{
  id            Int       @id   @default(autoincrement())
  puuID         String    @unique
  status        String    
  analyzed_at   DateTime?
  created_at  DateTime @default(now())
}

model matches_analyzed {
  id            Int       @id @default(autoincrement())
  match_id      String
  status        String
  analyzed_at   DateTime?
  created_at    DateTime @default(now())
}

model matches_info {
  id                  Int @id @default(autoincrement())
  match_id            String @unique
  endOfGameResult     String
  gameCreation        Int
  gameDuration        Int
  gameMode            String
  gameName            String
  gameStartTimestamp  Int
  gameType            String
  gameVersion         String
  mapId               Int
  championsTeam       String
  damagePerMinute     Float
  deaths              Int
  kills               Int
  assistis            Int
  dragonKilleds       Int
  heraldKilles        Int
  baronKilles         Int
  doubleKill          Int
  tripleKill          Int
  quadraKill          Int
  pentaKill           Int
  aces                Int
  turretsKilled       Int
  firstBlood          Boolean
  firstTower          Boolean
  spellCast           Int
  queueId             Int
  bans                String
  created_at          DateTime @default(now())

  
}